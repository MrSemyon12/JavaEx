public static void main(String[] args) {
        String s = "Денисенко Семен Денисович 25.03.2001 г.Хабаровск";
        System.out.println(isCorrect(s));
    }

    public static boolean isCorrect( String s ) {
        return s.matches("^[А-Я][а-я]{1,20}\\s[А-Я][а-я]{1,20}\\s[А-Я][а-я]{1,20}\\s\\d{2}\\.\\d{2}\\.\\d{4}\\sг\\.[А-Я][а-я]{1,20}$");
    }